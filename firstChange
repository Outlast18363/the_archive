    <span class="biFF1">public</span> <span class="biFF1">static</span> <span class="biFF1"><span style="color:#008855">void</span></span> <span class="">main</span><span class="KviDO">(</span><span style="color:#008855">String</span><span class="KviDO">[</span><span class="KviDO">]</span> args<span class="KviDO">)</span> throws IOException <span class="KviDO">{</span>
        Scanner <span class="biFF1">in</span> <span class="iYKHF"><span style="color:#981a1a">=</span></span> <span class="biFF1">new</span> <span class="">Scanner</span><span class="KviDO">(</span>System<span class="KviDO">.</span>in<span class="KviDO">)</span><span class="KviDO">;</span>
        ArrayList<span class="iYKHF"><span style="color:#981a1a">&lt;</span></span><span style="color:#008855">String</span><span class="iYKHF"><span style="color:#981a1a">&gt;</span></span> list <span class="iYKHF"><span style="color:#981a1a">=</span></span> <span class="biFF1">new</span> <span class="">ArrayList</span><span class="KviDO">(</span><span class="KviDO">)</span><span class="KviDO">;</span>
        <span class="biFF1">while</span><span class="KviDO">(</span><span class="biFF1">in</span><span class="KviDO">.</span><span class="">hasNext</span><span class="KviDO">(</span><span class="KviDO">)</span><span class="KviDO">)</span><span class="KviDO">{</span> <span class="u95Zk"><span style="color:#aa5500">//hasNext method here!</span></span>
            list<span class="KviDO">.</span><span class="">add</span><span class="KviDO">(</span><span class="biFF1">in</span><span class="KviDO">.</span><span class="">next</span><span class="KviDO">(</span><span class="KviDO">)</span><span class="KviDO">)</span><span class="KviDO">;</span>
        <span class="KviDO">}</span>
        
        <span class="biFF1">for</span><span class="KviDO">(</span><span style="color:#008855">String</span> s<span class="KviDO">:</span> list<span class="KviDO">)</span> System<span class="KviDO">.</span>out<span class="KviDO">.</span><span class="">println</span><span class="KviDO">(</span>s<span class="KviDO">)</span><span class="KviDO">;</span> <span class="u95Zk"><span style="color:#aa5500">//output the stored values</span></span>
    <span class="KviDO">}</span>
<span class="KviDO">}</span></span></pre><div type="pre" data-hook="rcv-block30"></div><div id="viewer-eqhjl" class="xVISr Y9Dpf bCMSCT OZy-3 lnyWN yMZv8w bCMSCT public-DraftStyleDefault-block-depth0 fixed-tab-size public-DraftStyleDefault-text-ltr"><span class="B2EFF public-DraftStyleDefault-ltr"><span><br role="presentation"/></span></span></div><div type="empty-line" data-hook="rcv-block31"></div><h2 id="viewer-1hu8d" class="qDGi6 Y9Dpf OZy-3 lnyWN yMZv8w bCMSCT public-DraftStyleDefault-block-depth0 fixed-tab-size public-DraftStyleDefault-text-ltr" style="line-height:1.225"><span class="B2EFF public-DraftStyleDefault-ltr"><span><strong></strong></span></span></h2><div type="h2" data-hook="rcv-block32"></div><h2 id="viewer-3ghd0" class="qDGi6 Y9Dpf OZy-3 lnyWN yMZv8w bCMSCT public-DraftStyleDefault-block-depth0 fixed-tab-size public-DraftStyleDefault-text-ltr" style="line-height:1.225"><span class="B2EFF public-DraftStyleDefault-ltr"><span><strong><span style="font-size:20px">Taking a further step, what if the lines have different lengths too?</span></strong></span></span></h2><div type="h2" data-hook="rcv-block33"></div><h2 id="viewer-fvde8" class="qDGi6 Y9Dpf OZy-3 lnyWN yMZv8w bCMSCT public-DraftStyleDefault-block-depth0 fixed-tab-size public-DraftStyleDefault-text-ltr" style="line-height:1.225"><span class="B2EFF public-DraftStyleDefault-ltr"><span><br role="presentation"/></span></span></h2><div type="empty-line" data-hook="rcv-block34"></div><h3 id="viewer-fetka" class="DjYhe Y9Dpf OZy-3 lnyWN yMZv8w bCMSCT public-DraftStyleDefault-block-depth0 fixed-tab-size public-DraftStyleDefault-text-ltr" style="line-height:1.43"><span class="B2EFF public-DraftStyleDefault-ltr"><span><strong>input</strong></span></span></h3><div type="h3" data-hook="rcv-block35"></div><h3 id="viewer-927od" class="DjYhe Y9Dpf OZy-3 lnyWN yMZv8w bCMSCT public-DraftStyleDefault-block-depth0 fixed-tab-size public-DraftStyleDefault-text-ltr" style="line-height:1.43"><span class="B2EFF public